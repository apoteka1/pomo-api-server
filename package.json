{
	"name": "pomo-server",
	"version": "1.0.0",
	"description": "backend for pomodoro timer",
	"main": "./dist/app.js",
	"scripts": {
		"build": "npx tsc",
		"start": "node dist/listen.js",
		"dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/listen.js\"",
		"setup-dbs": "psql -f ./db/setup.sql",
		"seed": "node ./dist/db/seeds/run-seed.js"
	},
