	"dependencies": {
		"@types/pg-format": "^1.0.2",
		"dotenv": "^16.0.0",
		"express": "^4.17.3",
		"nodemon": "^2.0.15",
		"pg": "^8.7.3",
		"pg-format": "^1.0.4"
	},
	"devDependencies": {
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.13",
		"@types/node": "^17.0.23",
		"@types/pg": "^8.6.5",
